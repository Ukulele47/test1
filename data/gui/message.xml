<?xml version="1.0" standalone="no" ?>
<!-- this is a mtp-target gui resource -->
<resource>
  <object class="CGuiFrame" name="messageFrame">
    <minSize>400,0</minSize>
    <position>0,0</position>
    <alignment>eAlignCenterHorizontal|eAlignCenterVertical</alignment>
    <element>
      <object class="CGuiVBox">
        <alignment>eAlignExpandHorizontal|eAlignExpandVertical</alignment>
        <element>

          <object class="CGuiText" name="msg">
            <string>GuiNewVersion</string>
          </object>

          <!-- OK / CANCEL -->
          <object class="CGuiSpacer">
            <minHeight>20</minHeight>
          </object>
          <object class="CGuiHBox">
            <alignment>eAlignExpandHorizontal|eAlignCenterVertical</alignment>
            <element>
              <object class="CGuiButton" name="bOk">
                <minSize>80,0</minSize>
                <element>
                  <object class="CGuiText">
                    <string>GuiOk</string>
                  </object>
                </element>
              </object>
              <object class="CGuiButton" name="bCancel">
                <minSize>80,0</minSize>
                <element>
                  <object class="CGuiText">
                    <string>GuiCancel</string>
                  </object>
                </element>
              </object>
            </element>
          </object>
        </element>
      </object>
    </element>
  </object>
</resource>
