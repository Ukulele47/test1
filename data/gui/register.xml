<?xml version="1.0" standalone="no" ?>
<!-- this is a mtp-target gui resource -->
<resource>
	<object class="CGuiFrame" name="registerFrame">
		<alignment>eAlignCenterHorizontal|eAlignCenterVertical</alignment>
		<minSize>0,120</minSize>
		<position>0,50</position>
		<element>
			<object class="CGuiVBox">
				<alignment>eAlignExpandHorizontal|eAlignExpandVertical</alignment>
				<element>
					<object class="CGuiHBox">
						<alignment>eAlignExpandHorizontal|eAlignCenterVertical</alignment>
						<element>
							<object class="CGuiText" name="loginText">
								<alignment>eAlignLeft|eAlignCenterVertical</alignment>
								<string>GuiLogin</string>
							</object>
							<object class="CGuiText" name="loginEntry">
								<alignment>eAlignRight|eAlignCenterVertical</alignment>
								<minSize>100,0</minSize>
								<isMultiline>false</isMultiline>
								<isEntry>true</isEntry>
								<isEditable>true</isEditable>
								<string></string>
							</object>
						</element>
					</object>
					<object class="CGuiHBox">
						<alignment>eAlignExpandHorizontal|eAlignCenterVertical</alignment>
						<element>
							<object class="CGuiText" name="passwordText">
								<alignment>eAlignLeft|eAlignCenterVertical</alignment>
								<string>GuiPassword</string>
							</object>
							<object class="CGuiText" name="passwordEntry">
								<alignment>eAlignRight|eAlignCenterVertical</alignment>
								<minSize>100,0</minSize>
								<isMultiline>false</isMultiline>
								<isEntry>true</isEntry>
								<isEditable>true</isEditable>
								<isPassword>true</isPassword>
								<string></string>
							</object>
						</element>
					</object>
          <object class="CGuiHBox">
            <alignment>eAlignExpandHorizontal|eAlignCenterVertical</alignment>
            <element>
              <object class="CGuiText" name="passwordText2">
                <alignment>eAlignLeft|eAlignCenterVertical</alignment>
                <string>GuiPasswordAgain</string>
              </object>
              <object class="CGuiText" name="passwordEntry2">
                <alignment>eAlignRight|eAlignCenterVertical</alignment>
                <minSize>100,0</minSize>
                <isMultiline>false</isMultiline>
                <isEntry>true</isEntry>
                <isEditable>true</isEditable>
                <isPassword>true</isPassword>
                <string></string>
              </object>
            </element>
          </object>
          <object class="CGuiHBox">
            <alignment>eAlignExpandHorizontal|eAlignCenterVertical</alignment>
            <element>
              <object class="CGuiText" name="emailText">
                <alignment>eAlignLeft|eAlignCenterVertical</alignment>
                <string>GuiEmail</string>
              </object>
              <object class="CGuiText" name="emailEntry">
                <alignment>eAlignRight|eAlignCenterVertical</alignment>
                <minSize>150,0</minSize>
                <isMultiline>false</isMultiline>
                <isEntry>true</isEntry>
                <isEditable>true</isEditable>
                <string></string>
              </object>
            </element>
          </object>
          <object class="CGuiHBox">
            <alignment>eAlignLeft|eAlignCenterVertical</alignment>
            <element>
              <object class="CGuiText">
                <string>GuiAcceptConditions</string>
              </object>
              <object class="CGuiCheck" name="acceptCheck" />
            </element>
          </object>
          <object class="CGuiHBox">
            <alignment>eAlignExpandHorizontal|eAlignBottom</alignment>
            <element>
              <object class="CGuiButton" name="bReadCondition">
                <minSize>80,0</minSize>
                <element>
                  <object class="CGuiText">
                    <string>GuiReadConditions</string>
                  </object>
                </element>
              </object>
            </element>
          </object>
          <object class="CGuiHBox">
						<alignment>eAlignExpandHorizontal|eAlignBottom</alignment>
						<element>
							<object class="CGuiButton" name="bCreate">
								<minSize>80,0</minSize><element><object class="CGuiText"><string>GuiCreate</string></object></element>
							</object>
              <object class="CGuiButton" name="bBack">
                <minSize>80,0</minSize>
                <element>
                  <object class="CGuiText">
                    <string>GuiBack</string>
                  </object>
                </element>
              </object>
            </element>
          </object>
        </element>
			</object>
		</element>
	</object>
</resource>
