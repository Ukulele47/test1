<?xml version="1.0" standalone="no" ?>
<!-- this is a mtp-target gui resource -->
<resource>
  <object class="CGuiFrame" name="settingsFrame">
    <minSize>400,0</minSize>
    <position>0,0</position>
    <alignment>eAlignCenterVertical|eAlignCenterHorizontal</alignment>
    <element>
      <object class="CGuiVBox">
        <alignment>eAlignExpandHorizontal|eAlignExpandVertical</alignment>
        <element>

          <!-- LANGUAGES -->
          <object class="CGuiButton" name="bLanguages">
            <minSize>80,0</minSize>
            <element>
              <object class="CGuiText">
                <string>GuiLanguage</string>
              </object>
            </element>
          </object>

          <object class="CGuiSpacer">
            <minHeight>20</minHeight>
          </object>
          <object class="CGuiText">
            <string>GuiVideo</string>
          </object>
          <object class="CGuiSpacer">
            <minHeight>5</minHeight>
          </object>

          <!-- SCREEN RESOLUTION -->
          <object class="CGuiVBox">
            <element>
              <object class="CGuiHBox">
                <alignment>eAlignLeft|eAlignCenterVertical</alignment>
                <element>
                  <object class="CGuiText">
                    <string>GuiScreenResolution</string>
                  </object>
                  <object class="CGuiText" name="resolutionText">
                    <string></string>
                    <isI18N>false</isI18N>
                  </object>
                </element>
              </object>
              <object class="CGuiHScale" name="resolutionHScale">
                <alignment>eAlignExpandHorizontal|eAlignCenterVertical</alignment>
              </object>
            </element>
          </object>

          <!-- FULLSCREEN -->
          <object class="CGuiHBox">
            <alignment>eAlignLeft|eAlignCenterVertical</alignment>
            <element>
              <object class="CGuiText">
                <string>GuiFullScreen</string>
              </object>
              <object class="CGuiCheck" name="FullscreenCheck" />
            </element>
          </object>

          <!-- VSYNC -->
          <object class="CGuiHBox">
            <alignment>eAlignLeft|eAlignCenterVertical</alignment>
            <element>
              <object class="CGuiText">
                <string>GuiVerticalSyncronization</string>
              </object>
              <object class="CGuiCheck" name="VSyncCheck" />
            </element>
          </object>

          <!-- ANTIALIAS -->
          <object class="CGuiHBox">
            <alignment>eAlignLeft|eAlignCenterVertical</alignment>
            <element>
              <object class="CGuiText">
                <string>GuiUseAntiAlias</string>
              </object>
              <object class="CGuiCheck" name="AntiAliasCheck" />
            </element>
          </object>

          <!-- CLOUDS -->
          <object class="CGuiHBox">
            <alignment>eAlignLeft|eAlignCenterVertical</alignment>
            <element>
              <object class="CGuiText">
                <string>GuiDisplayClouds</string>
              </object>
              <object class="CGuiCheck" name="DisplayCloudsCheck" />
            </element>
          </object>

          <object class="CGuiSpacer">
            <minHeight>20</minHeight>
          </object>
          <object class="CGuiText">
            <string>GuiMusicSound</string>
          </object>
          <object class="CGuiSpacer">
            <minHeight>5</minHeight>
          </object>

          <!-- MUSIC -->
          <object class="CGuiHBox">
            <alignment>eAlignExpandHorizontal|eAlignCenterVertical</alignment>
            <element>
              <object class="CGuiHBox">
                <alignment>eAlignLeft|eAlignCenterVertical</alignment>
                <element>
                  <object class="CGuiText">
                    <string>GuiMusic</string>
                  </object>
                  <object class="CGuiCheck" name="MusicCheck" />
                </element>
              </object>
              <object class="CGuiText">
                <string>GuiVolume</string>
              </object>
              <object class="CGuiTextPercent" name="MusicVolumePercentText">
                <alignment>eAlignLeft|eAlignCenterVertical</alignment>
                <minSize>40,0</minSize>
              </object>
              <object class="CGuiHScale" name="MusicVolumeHScale">
                <alignment>eAlignExpandHorizontal|eAlignCenterVertical</alignment>
                <minSize>0,30</minSize>
              </object>
            </element>
          </object>

          <!-- SOUND -->
          <object class="CGuiHBox">
            <alignment>eAlignExpandHorizontal|eAlignCenterVertical</alignment>
            <element>
              <object class="CGuiHBox">
                <alignment>eAlignLeft|eAlignCenterVertical</alignment>
                <element>
                  <object class="CGuiText">
                    <string>GuiSound</string>
                  </object>
                  <object class="CGuiCheck" name="SoundCheck" />
                </element>
              </object>
              <object class="CGuiText">
                <string>GuiVolume</string>
              </object>
              <object class="CGuiTextPercent" name="SoundVolumePercentText">
                <alignment>eAlignLeft|eAlignCenterVertical</alignment>
                <minSize>40,0</minSize>
              </object>
              <object class="CGuiHScale" name="SoundVolumeHScale">
                <alignment>eAlignExpandHorizontal|eAlignCenterVertical</alignment>
                <minSize>0,30</minSize>
              </object>
            </element>
          </object>

          <object class="CGuiSpacer">
            <minHeight>20</minHeight>
          </object>
          <object class="CGuiText">
            <string>GuiNetwork</string>
          </object>
          <object class="CGuiSpacer">
            <minHeight>5</minHeight>
          </object>

          <!-- UseUdp -->
          <object class="CGuiHBox">
            <alignment>eAlignLeft|eAlignCenterVertical</alignment>
            <element>
              <object class="CGuiText">
                <string>GuiUseUDP</string>
              </object>
              <object class="CGuiCheck" name="UseUdpCheck" />
            </element>
          </object>

          <!-- LAG COMPENSATION -->
          <object class="CGuiHBox">
            <alignment>eAlignExpandHorizontal|eAlignCenterVertical</alignment>
            <element>
              <object class="CGuiText">
                <string>GuiLagCompensation</string>
              </object>
              <object class="CGuiTextPercent" name="LagCompensationPercentText">
                <alignment>eAlignLeft|eAlignCenterVertical</alignment>
                <minSize>40,0</minSize>
              </object>
              <object class="CGuiHScale" name="LagCompensationHScale">
                <alignment>eAlignExpandHorizontal|eAlignCenterVertical</alignment>
                <minSize>0,30</minSize>
              </object>
            </element>
          </object>

          <object class="CGuiSpacer">
            <minHeight>20</minHeight>
          </object>
          <object class="CGuiText">
            <string>GuiMisc</string>
          </object>
          <object class="CGuiSpacer">
            <minHeight>5</minHeight>
          </object>

          <!-- LOG -->
          <object class="CGuiHBox">
            <alignment>eAlignLeft|eAlignCenterVertical</alignment>
            <element>
              <object class="CGuiText">
                <string>GuiLogChat</string>
              </object>
              <object class="CGuiCheck" name="LogChatCheck" />
            </element>
          </object>

          <!-- SAVE -->
          <object class="CGuiSpacer">
            <minHeight>20</minHeight>
          </object>
          <object class="CGuiHBox">
            <alignment>eAlignRight|eAlignBottom</alignment>
            <element>
              <object class="CGuiText">
                <string>GuiRestartGame</string>
              </object>
              <object class="CGuiButton" name="bSave">
                <minSize>80,0</minSize>
                <element>
                  <object class="CGuiText">
                    <string>GuiSave</string>
                  </object>
                </element>
              </object>
            </element>
          </object>

        </element>
      </object>
    </element>
  </object>
</resource>
