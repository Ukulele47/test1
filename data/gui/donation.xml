<?xml version="1.0" standalone="no" ?>
<!-- this is a mtp-target gui resource -->
<resource>
  <object class="CGuiFrame" name="donationFrame">
    <alignment>eAlignCenterHorizontal|eAlignCenterVertical</alignment>
    <minSize>0,0</minSize>
    <position>150,100</position>
    <element>
      <object class="CGuiVBox">
        <alignment>eAlignExpandHorizontal|eAlignExpandVertical</alignment>
        <element>

          <object class="CGuiText">
            <string>GuiDonationTitle</string>
          </object>
          <object class="CGuiSpacer">
            <minHeight>10</minHeight>
          </object>
          <object class="CGuiText" name="donationExplText">
            <string>GuiDonationExpl</string>
          </object>
          <object class="CGuiSpacer">
            <minHeight>10</minHeight>
          </object>
          <object class="CGuiText" name="donationGoal">
            <string>GuiDonationGoal</string>
          </object>
          <object class="CGuiText" name="donationGoalText">
            <string></string>
            <isI18N>false</isI18N>
          </object>
          <object class="CGuiProgressBar" name="FundRaisedProgressBar">
            <alignment>eAlignExpandHorizontal|eAlignCenterVertical</alignment>
          </object>
          <object class="CGuiButton" name="bDonate">
            <element>
              <object class="CGuiText">
                <string>GuiDonate</string>
              </object>
            </element>
          </object>

        </element>
      </object>
    </element>
  </object>
</resource>
